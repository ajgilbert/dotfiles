IgnoreUnknown RemoteCommand
Include ~/.ssh/local.config

Host *
  ServerAliveInterval 60
  XAuthLocation /opt/X11/bin/xauth

Host lxplus7.cern.ch lxtunnel.cern.ch lxp7 lxp9 lxplus9.cern.ch lxplus.cern.ch
User agilbert
ForwardX11 yes
ForwardX11Trusted yes
ForwardX11Timeout 8h
GSSAPIAuthentication yes
GSSAPIDelegateCredentials yes
#AddressFamily inet
Host lxp7
HostName lxplus7.cern.ch
Host lxp9
HostName lxplus.cern.ch

# CERN vm-cc7
Host agilbert-vm-cc7.cern.ch avm7
User agilbert
ForwardX11 yes
ForwardX11Trusted yes
ForwardX11Timeout 8h
ProxyJump lxtunnel.cern.ch
GSSAPIAuthentication yes
GSSAPIDelegateCredentials yes
#AddressFamily inet
Host avm7
HostName agilbert-vm-cc7.cern.ch


Host vm-cc7
RemoteCommand /afs/cern.ch/user/a/agilbert/launch-cmssw-el7 
HostName agilbert-vm-el9.cern.ch
RequestTTY yes

Host agilbert-vm-el9.cern.ch avm9
User agilbert
ForwardX11 yes
ForwardX11Trusted yes
ForwardX11Timeout 8h
ProxyJump lxtunnel.cern.ch
GSSAPIAuthentication yes
GSSAPIDelegateCredentials yes
Host avm9
HostName agilbert-vm-el9.cern.ch

Host github.com
IdentityFile ~/.ssh/id_rsa_github

Host llrgate01.in2p3.fr llrgate
User gilbert
ForwardAgent yes
ForwardX11 yes
ForwardX11Trusted yes
ForwardX11Timeout 8h
